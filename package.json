{
  "name": "hls2vod",
  "version": "0.0.1",
  "private": true,
  "homepage": "./",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/electron.webpack.js --mode development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack/react.webpack.js --mode development",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/electron.webpack.js",
    "build:react": "webpack --config webpack/react.webpack.js",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder -lw",
    "electron-pack": "build --em.main=build/electron.js",
    "test": "jest"
  },
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.19.2",
    "chalk": "^4.1.0",
    "cli-progress": "^3.8.2",
    "commander": "^4.1.1",
    "cookie-parser": "~1.4.4",
    "csvtojson": "^2.0.10",
    "debug": "~2.6.9",
    "fs-extra": "^8.1.0",
    "hidefile": "^3.0.0",
    "jquery": "^3.5.1",
    "m3u8-parser": "^4.4.3",
    "materialize-css": "^1.0.0",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "nedb": "^1.8.0",
    "number-magic": "^1.3.0",
    "p-queue": "^6.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lazylog": "^4.5.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1",
    "trash": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.12.0",
    "@babel/runtime": "^7.11.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^8.1.1",
    "@types/jest": "^26.0.15",
    "@types/jquery": "^3.5.1",
    "@types/materialize-css": "^1.0.8",
    "@types/nedb": "^1.8.11",
    "@types/node": "^13.13.21",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.0",
    "acorn": "^7.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "electron": "^8.5.2",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.1.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^26.6.1",
