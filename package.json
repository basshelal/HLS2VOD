    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "build": {
    "asar": false,
    "extends": null,
    "appId": "com.github.basshelal.hls2vod",
    "directories": {
      "output": "packaged/${os}"
    }
  }
}
